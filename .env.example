# .env.example - Template de variáveis de ambiente
# Copie este arquivo para .env e preencha com seus valores reais

# Django
SECRET_KEY=cole-uma-secret-key-gerada-aqui
DEBUG=False

# Allowed Hosts (separe por vírgulas, sem espaços)
ALLOWED_HOSTS=localhost,127.0.0.1

# Database PostgreSQL
DB_NAME=nome_do_banco
DB_USER=usuario_do_postgres
DB_PASSWORD=senha_do_postgres
DB_HOST=localhost
DB_PORT=5432

# Gemini API Key (Google AI)
# Obtenha em: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=cole-sua-chave-gemini-aqui